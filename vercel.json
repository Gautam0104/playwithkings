{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "www",
  "public": true,
  "cleanUrls": true,
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/build/(.*)",
      "dest": "/build/$1"
    },
    {
      "src": "/manifest.json",
      "dest": "/manifest.json"
    },
    {
      "src": "/cordova.js",
      "dest": "/cordova.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
} 